<% include ../layouts/head %>

<!-- Styles for Karyo.js -->
<link rel="stylesheet" type="text/css" href="<%= site.vendor %>karyojs/css/font.css">
<link rel="stylesheet" type="text/css" href="<%= site.vendor %>karyojs/css/style.css">

<!-- JavaScript vendor -->
<script type="text/javascript" src="<%= site.vendor %>karyojs/vendor/jquery/jquery.min.js"></script>
<script type="text/javascript" src="<%= site.vendor %>karyojs/vendor/svgjs/svg.min.js"></script>

<!-- Javascript Karyo.js -->
<script type="text/javascript" src="<%= site.vendor %>karyojs/js/karyo.js"></script>


<!-- Styles for GenomeMaps -->
<link rel="stylesheet" href="<%= site.vendor %>gmaps/vendor/fontawesome/css/font-awesome.min.css">
<link rel="stylesheet" href="<%= site.vendor %>gmaps/vendor/qtip2/jquery.qtip.min.css">
<link rel="stylesheet" href="<%= site.vendor %>gmaps/styles/css/style.css">

<!-- Scripts for Genome Maps-->
<script type="text/javascript" src="<%= site.vendor %>gmaps/vendor/underscore/underscore-min.js"></script>
<script type="text/javascript" src="<%= site.vendor %>gmaps/vendor/backbone/backbone.js"></script>
<script type="text/javascript" src="<%= site.vendor %>gmaps/vendor/jquery/dist/jquery.min.js"></script>
<script type="text/javascript" src="<%= site.vendor %>gmaps/vendor/qtip2/jquery.qtip.min.js"></script>
<script type="text/javascript" src="<%= site.vendor %>gmaps/vendor/uri.js/src/URI.min.js"></script>
<script type="text/javascript" src="<%= site.vendor %>gmaps/gv-config.js"></script>
<script type="text/javascript" src="<%= site.vendor %>gmaps/genome-viewer.js"></script>


<!-- Styles for siimple-tables -->
<!-- <link rel="stylesheet" type="text/css" href="<%= site.vendor %>siimple-tables/css/font.css"> -->
<link rel="stylesheet" type="text/css" href="<%= site.vendor %>siimple-tables/css/style.css">

<!-- JavaScript vendor for siimple-tables -->
<!-- <script type="text/javascript" src="<%= site.vendor %>siimple-tables/vendor/jquery/jquery.min.js"></script> -->
<script type="text/javascript" src="<%= site.vendor %>siimple-tables/vendor/objectsort/objectsort.js"></script>

<!-- Javascript siimple-tables -->
<script type="text/javascript" src="<%= site.vendor %>siimple-tables/js/jtables.min.js"></script>

<!-- JavaScript GetQuery -->
<script type="text/javascript" src="<%= site.vendor %>getquery/getquery.js"></script>


<!-- Project -->
<script type="text/javascript">var projectId = "<%= projectId %>"; </script>

<!-- Dashboard scripts -->
<script type="text/javascript" src="/js/dashboard.js"></script>
<script type="text/javascript" src="/js/karyo.js"></script>
<script type="text/javascript" src="/js/gmaps.js"></script>

<!-- Dashboard panel -->
<div class="dashboard main">

  <!-- Dashboard left -->
  <div class="dashboard-left">

    <!-- Filter -->
		<div class="filters">

      <!-- Filter by number of reads -->
      <b>Filter by number of reads</b><br>
      Show only the positions that have the minimum number of reads:

      <!-- Input -->
      <input id="input-reads" type="text" class="form-input" value="5">

      <!-- Update button -->
      <div class="btn btn-blue btn-small" id="filter-reads">Filter</div>

      <!-- Line -->
      <hr>

      <!-- Find gene -->
      <b>Find Cancer Genes</b><br>
      Find for genes implicated in cancer.

      <!-- Input -->
      <input id="input-genes" type="text" class="form-input" placeholder="Gene">

      <!-- Find button -->
      <div class="btn btn-blue btn-small" id="find-genes">Find</div>

      <!-- View the full list -->
      <div class="btn btn-small btn-green" id="full-genes">Full list</div>

      <!-- Line -->
      <hr>

      <!-- Find Tumour -->
      <b>Find genes by Tumour Type</b><br>
      Gives you a list with all genes involved in a seleted tumour type (somatic and germline):

      <!-- Input -->
      <input id="input-tumours" type="text" class="form-input" placeholder="Tumour">

      <!-- Find button -->
      <div class="btn btn-blue btn-small" id="find-tumours">Find</div>

      <!-- View the full list -->
      <div class="btn btn-small btn-green" id="full-tumours">Full list</div>

      <!-- Line -->
      <hr>

      <!-- Generate a report -->
      <b>Report</b><br>
      Generate a report with all the integration sites.<br>

      <!-- Link -->
      <a href="/report/<%= projectId %>" class="btn btn-blue btn-small" target="_blank">Generate</a>

    </div>

  </div>

  <!-- Dashboard right -->
  <div class="dashboard-right">

    <!-- Project status -->
    <div class="dashboard-status" align="center">

      <!-- Project title -->
      <h1><%= projectTitle %></h1>

      <!-- Aligner information -->
      Aligner: <b><%= projectAligner %></b> (in <b><%= projectTime %> seconds</b>).
      Reference genome: <b><%= projectSpecie %></b>.

      <!-- Reads status -->
      Reads: <b><%= projectSeqOrig %></b>. Mapped: <b><%= projectSeqMapp %></b>.<br>

      <!-- Project information -->
      Remaining days: <b><%= projectDays %></b> [<a href="/project/<%= projectId %>/extend">Extend</a>].<br>

    </div>

    <!-- Div for alerts -->
    <div id="alert_div"></div>

    <!-- Div for tables -->
    <div id="table_div"></div>

    <!-- Karyo div -->
		<div id="karyo_div" class="dashboard-right-karyo"></div>

    <!-- Genome Maps div -->
    <div id="application" class="dashboard-right-gmaps"></div>

  </div>

<!-- Close dashboard div -->
</div>


<% include ../layouts/foot %>
